# Ignoring backup files
*~

# Ignoring kdevelop4 project files
*.kdev4

# Ignoring vim restore files
*.swp

# Ignoring kde4 .directory files
.directory

# Ignoring user defined controllers, models and views
controllers/*
!controllers/empty

models/*
!models/empty
!models/dbsources

models/dbsources/*
!models/dbsources/empty

views/*
!views/empty
!views/layouts
!views/helpers

views/helpers/*
!views/helpers/empty

views/layouts/*
!views/layouts/empty

# Ignoring user files in webroot
webroot/js/*
!webroot/js/empty

webroot/img/*
!webroot/img/empty

webroot/css/*
!webroot/css/empty
